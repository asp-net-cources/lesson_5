@{
    ViewData["Title"] = "Home Page";
    var orderNames = new []{ "Шарик", "Кошка", "Водка" };
    var orderDescriptions = new []{ "Голубой", "Персидская", "Холодная" };
    var orderCount = new []{ 5, 3, 50 };
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <table>
        <tr>
            <th>Название</th>
            <th>Описание</th>
            <th>Количество</th>
        </tr>
        @for (int i = 0; i < 3; i++)
        {
            <tr>
                <td>@orderNames[i]</td>
                <td>@orderDescriptions[i]</td>
                <td>@orderCount[i]</td>
            </tr>
        }
    </table>
</div>